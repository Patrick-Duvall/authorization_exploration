<h1>All Users</h1>

<% if @user == nil %>
  <%= link_to "Log In", login_path %>
<% else %>
<h1>Welcome <%= @user.name %>, you are logged in!</h1>

<%= link_to "Log Out", logout_path, method: :delete %>

<% end %>



<ul>
  <% @users.each do |user| %>
    <li><%= user.name %></li>
    <li><%= user.email %></li>
  <% end %>
</ul>


<%= link_to "Register as User", new_user_path %>
